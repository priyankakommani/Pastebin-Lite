generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paste {
  id             String    @id @default(cuid())
  content        String
  maxViews       Int?
  remainingViews Int?
  expiresAt      DateTime?
  createdAt      DateTime  @default(now())
}
